{
  "name": "next-export-i18n_example",
  "version": "1.3.0",
  "description": "Internationalize next.js with true support for next export. Example page",
  "keywords": [
    "next.js",
    "react.js",
    "typescript",
    "i18n",
    "export",
    "ssg"
  ],
  "author": "Martin Krause <oss@mkrause.info> (http://mkrause.info/)",
  "license": "MIT",
  "scripts": {
    "dev": "yarn yaml2json && next dev",
    "build": "yarn yaml2json && next build",
    "start": "next start",
    "lint": "next lint",
    "yaml2json": "npx yaml2json ./i18n --pretty --save --recursive",
    "export": "yarn yaml2json && next build && next export && npx serve ./out"
  },
  "dependencies": {
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.0.0",
    "@testing-library/react-hooks": "^7.0.1",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^26.0.24",
    "@types/react": "17.0.15",
    "eslint": "7.32.0",
    "eslint-config-next": "11.0.1",
    "jest": "^27.0.6",
    "next-export-i18n": "latest",
    "ts-jest": "^27.0.4",
    "typescript": "4.3.5",
    "yamljs": "^0.3.0"
  }
}